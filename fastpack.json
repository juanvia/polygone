{
  "entryPoints": [
    "lib/js/src/index.js"
  ],
  "preprocess": [
    {
      "re": "^src.+\\.js$",
      "process": "babel-loader"
    },
    {
      "re": "\\.svg$",
      "process": "url-loader"
    },
    {
      "re": "\\.png$",
      "process": "url-loader"
    },
    {
      "re": "\\.css$",
      "process": "style-loader!css-loader"
    }
  ]
}